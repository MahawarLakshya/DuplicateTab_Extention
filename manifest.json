{
  "manifest_version": 3,
  "name": "Duplicate Tab Shortcut (Ctrl+Shift+K)",
  "version": "1.0",
  "description": "Duplicates the current tab when Ctrl+Shift+K is pressed.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "duplicate-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "Duplicate the current tab"
    }
  }
}